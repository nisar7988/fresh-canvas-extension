{
  "manifest_version": 3,
  "name": "Fresh Canvas - Wallpaper Extension",
  "version": "1.0.0",
  "description": "Beautiful rotating wallpapers for your new tab page",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://images.unsplash.com/*"
  ]
}